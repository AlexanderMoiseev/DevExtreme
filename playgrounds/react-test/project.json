{
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "name": "react-test",
    "sourceRoot": "playgrounds/react-test/src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "@nrwl/webpack:webpack",
            "outputs": ["{options.outputPath}"],
            "defaultConfiguration": "production",
            "options": {
                "compiler": "babel",
                "outputPath": "dist/playgrounds/react-test",
                "index": "playgrounds/react-test/src/index.html",
                "baseHref": "/",
                "main": "playgrounds/react-test/src/main.tsx",
                "polyfills": "playgrounds/react-test/src/polyfills.ts",
                "tsConfig": "playgrounds/react-test/tsconfig.app.json",
                "assets": [
                    "playgrounds/react-test/src/favicon.ico",
                    "playgrounds/react-test/src/assets"
                ],
                "styles": ["playgrounds/react-test/src/styles.scss"],
                "scripts": [],
                "webpackConfig": "@nrwl/react/plugins/webpack"
            },
            "configurations": {
                "development": {
                    "extractLicenses": false,
                    "optimization": false,
                    "sourceMap": true,
                    "vendorChunk": true
                },
                "production": {
                    "fileReplacements": [
                        {
                            "replace": "playgrounds/react-test/src/environments/environment.ts",
                            "with": "playgrounds/react-test/src/environments/environment.prod.ts"
                        }
                    ],
                    "optimization": true,
                    "outputHashing": "all",
                    "sourceMap": false,
                    "namedChunks": false,
                    "extractLicenses": true,
                    "vendorChunk": false
                }
            }
        },
        "serve": {
            "executor": "@nrwl/webpack:dev-server",
            "defaultConfiguration": "development",
            "options": {
                "buildTarget": "react-test:build",
                "hmr": true
            },
            "configurations": {
                "development": {
                    "buildTarget": "react-test:build:development"
                },
                "production": {
                    "buildTarget": "react-test:build:production",
                    "hmr": false
                }
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": [
                    "playgrounds/react-test/**/*.{ts,tsx,js,jsx}"
                ]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "playgrounds/react-test/jest.config.ts",
                "passWithNoTests": true
            }
        }
    },
    "tags": []
}
