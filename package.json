{
    "private": true,
    "type": "module",
    "devDependencies": {
        "@nrwl/angular": "^15.0.13",
        "@nrwl/cypress": "^15.0.13",
        "@nrwl/eslint-plugin-nx": "^15.0.13",
        "@nrwl/jest": "15.0.13",
        "@nrwl/nx-cloud": "latest",
        "@nrwl/react": "15.0.13",
        "@nrwl/web": "15.0.13",
        "@testing-library/react": "13.4.0",
        "@types/jest": "28.1.1",
        "@types/node": "18.7.18",
        "@types/react": "18.0.20",
        "@types/react-dom": "18.0.6",
        "@types/react-router-dom": "5.3.3",
        "@typescript-eslint/eslint-plugin": "^5.27.1",
        "@typescript-eslint/parser": "^5.27.1",
        "babel-jest": "28.1.1",
        "cypress": "^10.11.0",
        "eslint": "^8.17.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-import-resolver-typescript": "^3.5.2",
        "eslint-plugin-cypress": "^2.10.3",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jsx-a11y": "6.6.1",
        "eslint-plugin-react": "7.31.8",
        "eslint-plugin-react-hooks": "4.6.0",
        "jest": "28.1.1",
        "jest-environment-jsdom": "28.1.1",
        "jest-preset-angular": "^12.2.2",
        "nx": "15.0.10",
        "react-test-renderer": "18.2.0",
        "rimraf": "^3.0.2",
        "ts-jest": "28.0.5",
        "ts-node": "10.9.1",
        "typescript": "^4.7.3"
    },
    "scripts": {
        "build:angular": "npm run build --prefix packages/angular",
        "build:core": "npm run build --prefix packages/core",
        "build:react": "npm run build --prefix packages/react",
        "build:packages": "npm run build:core && npm run build:react && npm run build:angular",
        "build": "nx run-many --target=build --projects=@devexpress/core,@devexpress/react,@devexpress/angular --skip-nx-cache",
        "watch": "nx run-many --target=watch",
        "clean": "rimraf ./{packages,playgrounds}/*/{lib,dist,tsc-out,*.tsbuildinfo}",
        "test": "nx run-many --target=test",
        "test:e2e": "nx run-many --target=e2e",
        "test:jest": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.config.json --no-cache",
        "lint": "npm run lint:eslint",
        "lint:eslint": "eslint --ignore-path .gitignore {packages,playgrounds}/*/src/**/*.ts",
        "lint:eslint:fix": "eslint --ignore-path .gitignore --fix {packages,playgrounds}/*/src/**/*.ts",
        "tsc:build": "tsc --build ./tsconfig.project.json",
        "tsc:watch": "npm run tsc:build -- --watch",
        "update:tsconfig": "ts-node-esm scripts/update-package-tsconfig.ts"
    },
    "workspaces": [
        "packages/*",
        "playgrounds/react",
        "playgrounds/react-test"
    ],
    "dependencies": {
        "core-js": "^3.6.5",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-router-dom": "6.4.1",
        "regenerator-runtime": "0.13.7",
        "tslib": "^2.3.0"
    }
}
